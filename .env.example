# Copy this file to .env and fill in your API keys
# These are public environment variables that will be exposed to the browser
# Use PUBLIC_ prefix for client-side variables in Astro

# Mapbox API Token (get from https://www.mapbox.com/)
PUBLIC_MAPBOX_TOKEN=your_mapbox_token_here

# Google Maps API Key (optional - get from https://console.cloud.google.com/)
PUBLIC_GOOGLE_MAPS_KEY=your_google_maps_key_here

# Geoapify API Key (optional - get from https://www.geoapify.com/)
PUBLIC_GEOAPIFY_KEY=your_geoapify_key_here

# CareCru Widget Settings (when ready to implement)
PUBLIC_CARECRU_LOCATION_ID=your_location_id_here
PUBLIC_CARECRU_PRACTICE_ID=your_practice_id_here